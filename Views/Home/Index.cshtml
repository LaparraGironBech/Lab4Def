@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using System.Data;
@*@model DataTable*@
@{
    Layout = null;
}
@model IEnumerable<Lab4.Models.Developer>

@{
    ViewData["Title"] = "IndexVer";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Carga de datos de developer</title>
</head>
<body>
    <style>
        .upload-boximport {
            font-size: 16px;
            background: white;
            border-radius: 50px;
            box-shadow: 5px 5px 10px black;
            width: 100px;
            height: 35px;
            outline: none;
        }

        .upload-box {
            font-size: 16px;
            background: white;
            border-radius: 50px;
            box-shadow: 5px 5px 10px black;
            width: 300px;
            height: 35px;
            outline: none;
        }

        ::-webkit-file-upload-button {
            color: white;
            background: #206a5d;
            padding: 6px;
            border: none;
            border-radius: 50px;
            box-shadow: 1px 0 1px 1px #6b4559;
            outline: none;
        }

            ::-webkit-file-upload-button:hover {
                background: #438a5e;
            }

        .upload-boximport:hover {
            background: #438a5e;
        }

        .Botones {
            width: 100px;
            height: 50px;
            color: white;
            background: #ff6a10;
            border: none;
            border-radius: 2px;
        }

            .Botones:hover {
                -webkit-transform: scale(1.1);
                -moz-transform: scale(1.1);
                -o-transform: scale(1);
                -webkit-transition-duration: 0.5s;
                -moz-transition-duration: 0.5s;
                -o-transition-duration: 0.5s;
            }
    </style>


    <h1>Carga tu base de datos</h1>
    <form asp-controller="Developers" asp-action="Index" method="post" enctype="multipart/form-data">

        <input type="file" class="upload-box" name="postedFile" />

        <input type="submit" class="upload-boximport" value="Import" />


    </form>


</body>

</html>
